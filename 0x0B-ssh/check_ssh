 #!/usr/bin/env bash

 # Find the path of the private key file
 private_key=$(find "$HOME" -name "school" -type f -print -quit 2>/dev/null)

 # Check if the private key file exists
 if [ -n "$private_key" ]; then
	     echo "Private key file 'school' exists at: $private_key"
     else
	         echo "Private key file 'school' does not exist."
 fi

